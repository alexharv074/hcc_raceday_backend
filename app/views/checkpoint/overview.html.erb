    <H1 >
    </H1 >
    <table class='table table-striped table-bordered' >
      <thead >
        <tr >
          <th >
            Checkpoint
          </th >
          <th >
            Due Soon
          </th >
          <th >
            At Checkpoint
          </th >
          <th >
            Withdrawn
          </th >
          <th >
            Calculated Average Time (minutes)
          </th >
        </tr >
      </thead >
      <% @rawdata['___orderedcheckpoints'].each do |chkpt, prevChkpt| %>
        <tr >
          <td >
            <%= chkpt %>
          </td >
          <td >
            <%= (prevChkpt.nil? || @count[prevChkpt].nil?)?'0':@count[prevChkpt]['OUT'] %>
          </td >
          <td >
            <%= (@count[chkpt].nil?)?'0':@count[chkpt]['IN'] %>
          </td >
          <td >
            <%= (@count[chkpt].nil?)?'0':@count[chkpt]['WD'] %>
          </td >
          <td >
            <%= (@averages[chkpt].nil?)?'0':(@averages[chkpt]/60) %>
          </td >
        </tr >
      <% end %>
    </table >
